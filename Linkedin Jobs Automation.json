        "jsCode": "const jobTitle = $json[\"Job Title\"] || \"Software Engineer\";\nconst jobLocation = $json[\"Job Location\"] || \"United States\";\nconst jobTypeInput = ($json[\"Job Type\"] || \"Full-time\").toLowerCase();\n\nconst jobTypeMap = {\n  \"full-time\": \"F\",\n  \"part-time\": \"P\",\n  \"internship\": \"I\",\n  \"temporary\": \"T\",\n  \"contract\": \"C\",\n  \"volunteer\": \"V\",\n  \"other\": \"O\"\n};\n\nconst jobTypeCode = jobTypeMap[jobTypeInput] || \"F\";\n\nconst encodedTitle = encodeURIComponent(jobTitle.trim());\nconst encodedLocation = encodeURIComponent(jobLocation.trim());\n\nconst searchUrl = `https://www.linkedin.com/jobs/search/?keywords=${encodedTitle}&location=${encodedLocation}&f_JT=${jobTypeCode}`;\n\nreturn [{ json: { searchUrl } }];\n"
      },
